{
  "name": "vroom",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "axios": "^0.21.1",
    "concurrently": "^6.2.1",
    "cors": "^2.8.5",
    "electron": "^13.2.2",
    "express": "^4.17.1",
    "nodemon": "^2.0.12",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "selenium-webdriver": "^4.0.0-beta.4",
    "wait-on": "^6.0.0",
    "web-vitals": "^1.1.2"
  },
  "main": "src/application.js",
  "scripts": {
    "start": "nodemon --exec react-scripts start",
